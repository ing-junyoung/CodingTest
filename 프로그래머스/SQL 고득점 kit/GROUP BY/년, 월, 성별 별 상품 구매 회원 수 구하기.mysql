# 내 답안

SELECT YEAR , MONTH , GENDER , COUNT(DISTINCT USER_ID) AS USERS
FROM(SELECT A.USER_ID , A.GENDER , DATE_FORMAT(B.SALES_DATE , '%Y') AS YEAR ,
DATE_FORMAT(B.SALES_DATE , '%m') AS MONTH
FROM USER_INFO AS A JOIN ONLINE_SALE AS B ON A.USER_ID = B.USER_ID
WHERE GENDER IS NOT NULL) AS C
GROUP BY YEAR , MONTH , GENDER
ORDER BY YEAR , MONTH , GENDER
