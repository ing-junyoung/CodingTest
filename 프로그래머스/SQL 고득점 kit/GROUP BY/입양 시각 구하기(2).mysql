# 내 답안

WITH RECURSIVE TBL2 AS(
SELECT 0 AS HOUR , 0 AS COUNT
UNION ALL
SELECT HOUR+1 , 0 AS COUNT FROM TBL2 WHERE HOUR < 23)

SELECT A.HOUR , COUNT(HOUR(B.DATETIME)) AS COUNT
FROM TBL2 AS A
LEFT OUTER JOIN ANIMAL_OUTS AS B ON A.HOUR = HOUR(B.DATETIME)
GROUP BY A.HOUR;
