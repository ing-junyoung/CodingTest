# 문제 링크 : https://school.programmers.co.kr/learn/courses/30/lessons/131117

# 내 답안

SELECT A.PRODUCT_ID , B.PRODUCT_NAME , SUM(A.AMOUNT*B.PRICE)AS TOTAL_SALES 
FROM FOOD_ORDER AS A LEFT JOIN FOOD_PRODUCT AS B ON A.PRODUCT_ID = B.PRODUCT_ID
WHERE A.PRODUCE_DATE LIKE '2022-05%' AND B.PRODUCT_ID IS NOT NULL
GROUP BY PRODUCT_ID
ORDER BY TOTAL_SALES DESC , A.PRODUCT_ID ASC
